{
  "manifest_version": 3,
  "name": "crxs",
  "version": "0",
  "description": "You can add extensions to your likes list",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "contextMenus",
    "sidePanel",
    "storage",
    "system.display",
    "unlimitedStorage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "action": {}
}